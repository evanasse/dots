[user]
    email = etienne.vanasse@outlook.com
    name = evanasse

[pull]
    rebase = true

[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    light = false
    syntax-theme = none
    minus-style = "#BF472C" normal
    minus-emph-style = black "#BF472C" bold
    line-numbers-minus-style = "#BF472C"
    plus-style = "#616655" normal
    plus-emph-style = white "#616655" bold
    line-numbers-plus-style = "#616655"
    line-numbers = true
    line-numbers-left-style = "#6b4035"
    line-numbers-right-style = "#6b4035"
    line-numbers-zero-style = "#6b4035"


[merge]
    conflictstyle = diff3

[diff]
    colorMoved = default
